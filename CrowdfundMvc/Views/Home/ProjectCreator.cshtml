@{
    ViewData["Title"] = "Project Creator";
}

<style>
    .imgbg {
        background-image: url(https://images.pexels.com/photos/37728/pexels-photo-37728.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940);
        background-repeat: no-repeat;
        background-size: cover;
        margin: 0;
        top: 0;
        padding-top: 15%;
        padding-bottom: 15%;
        font-family: 'Permanent Marker', cursive;
        color: white;
        font-size: 59px;
    }
</style>






<div class="imgbg">
    <div class="text-center">
        <p>Creator</p>
    </div>
</div>
   
<div class="text-center" style="margin-top: -50px;">
    <div class="row">
        <div class="col">
        </div>
        <div class="col">
        </div>
        <div class="col" style="padding-right: 50px;">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">Register</h5>
                    <p class="card-text">Register as a project creator and make your dream come true</p>
                    <a href="#" onclick="RegisterCreator()" class="btn btn-primary">Register</a>
                </div>
            </div>
        </div>
        <div class="col" style="padding-right: 50px;">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <h5 class="card-title">Login</h5>
                    <p class="card-text">
                        If you have registered, sign in with your mail                        .
                    </p>
                    <a href="#" onclick="LoginCreator()" class="btn btn-primary">Login</a>
                </div>
            </div>
        </div>
        <div class="col">
        </div>
        <div class="col">
        </div>
    </div>
</div>











