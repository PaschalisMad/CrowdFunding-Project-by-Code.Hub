@{
    ViewData["Title"] = "LoginBacker";
}


<h1 align="center">Login Backer</h1>

<form id="LoginBacker">
    <div class="form-group row">
        <label for="Email" class="col-sm-2 col-form-label">Email</label>
        <div class="col-sm-10">
            <input type="text" class="form-control" id="Email" placeholder="Email">
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-10">
            <input class="btn btn-primary" type="button" id="loginBackerButton" value="Submit">
        </div>
    </div>
</form>


<div id="responseDiv">   </div>
